[gui]
	encoding = utf-8
[user]
	email = aereal@kerare.org
	name = AOKI Hanae
[github]
	user = aereal
[alias]
	st = status -s
	df = diff
	co = checkout
	fixup = commit --amend -C HEAD
	svn-push = !git stash && git svn dcommit && git stash pop
	svn-pull = !git stash && git svn rebase && git stash pop
	dfst = diff --staged
[color]
	diff = auto
	branch = auto
	status = auto
	interactive = auto
[color "branch"]
	current = red
	local = yellow
	remote = green
[color "status"]
	added = yellow
	changed = green
	untracked = cyan
[color "diff"]
	commit = black white bold
	meta = blue
	frag = black
	old = red
	new = green
[i18n]
	commitencoding= utf-8
[core]
	editor = vim -u NONE -i NONE -N -c 'syntax on'
