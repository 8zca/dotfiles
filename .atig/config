# vim:set ft=ruby:

Atig::Gateway::Session.commands = %w(
	Retweet
	Reply
	User
	Favorite
	Destroy
	Status
	Whois
	Option
	Limit
).map {|command| Atig::Command.const_get(command) }

Atig::Gateway::Session.agents = %w(
	List
	ListStatus
	Following
	Mention
	Timeline
).map {|api| Atig::Agent.const_get(api) }

Atig::Gateway::Session.ifilters = %w(
	ExpandUrl
	Retweet
	Tid
).map {|filter| Atig::IFilter.const_get(filter) }

Atig::Gateway::Session.ofilters = %w(
	ShortUrl
).map {|filter| Atig::OFilter.const_get(filter) }

Atig::Gateway::Session.channels = %w(
	Timeline
	Mention
	Retweet
	List
).map {|channel| Atig::Channel.const_get(channel) }
